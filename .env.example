BACKEND_BASE_URL=https://api.example.com
RELAY_TOKEN=replace-me

# Optional
RELAY_INSTANCE_ID=
RELAY_MAX_TASKS=5
RELAY_WAIT_SECONDS=2147468
RELAY_TASK_TIMEOUT_MS=9999999
RELAY_CONCURRENCY=1
RELAY_PUSH_PORT=18790
RELAY_PUSH_PATH=/relay/messages

# Dev logging (hard-disabled in production)
RELAY_DEV_LOG=1
# Force dev logs even when NODE_ENV=production
RELAY_DEV_LOG_FORCE=1
RELAY_DEV_LOG_TEXT_MAXLEN=200
# Very noisy: logs raw-ish gateway WebSocket frames
RELAY_DEV_LOG_GATEWAY_FRAMES=1

# OpenClaw Gateway
OPENCLAW_GATEWAY_WS_URL=ws://127.0.0.1:18789
OPENCLAW_CONFIG_PATH=
OPENCLAW_STATE_DIR=
OPENCLAW_GATEWAY_TOKEN=
OPENCLAW_GATEWAY_PASSWORD=
OPENCLAW_SCOPES=operator.admin

# Optional STT (voice-to-text).
# STT_PROVIDER=deepgram
DEEPGRAM_API_KEY=
