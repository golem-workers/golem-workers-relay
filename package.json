{
  "name": "golem-workers-relay",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "test": "vitest run",
    "test:e2e": "vitest run -c vitest.e2e.config.ts --reporter=verbose",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",

    "openclaw:setup": "bash scripts/openclaw/onboard-docker.sh openclaw.env",
    "openclaw:up": "docker compose --env-file openclaw.env up -d --build openclaw-gateway",
    "openclaw:down": "docker compose --env-file openclaw.env down --remove-orphans",
    "openclaw:logs": "docker compose --env-file openclaw.env logs -f --tail=200 openclaw-gateway",
    "openclaw:ps": "docker compose --env-file openclaw.env ps",
    "openclaw:cli": "docker compose --env-file openclaw.env --profile cli run --rm --build openclaw-cli",
    "openclaw:reset": "docker compose --env-file openclaw.env down --remove-orphans && rm -rf .openclaw openclaw-workspace"
  },
  "dependencies": {
    "dotenv": "^17.3.1",
    "json5": "^2.2.3",
    "pino": "^10.3.1",
    "ws": "^8.18.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.8.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}

